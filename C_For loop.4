//print all lower case alphabet from starting to end

#include <stdio.h>
int main(){
char ch;
for(ch='a';ch<='z';ch++){
printf("%c\n",ch); }
return 0;
}

      (OR)

//print all upper case alphabet from starting to end

#include <stdio.h>
int main(){
char ch;
for(ch='A';ch<='Z';ch++){
printf("%c\n",ch); }
return 0;
}

    
